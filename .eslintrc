{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended", 
    "plugin:@typescript-eslint/recommended", 
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021, 
    "sourceType": "module"
  },
  "rules":{
  "import/no-extraneous-dependencies": "[error, { devDependencies: true }]"
  }
}
